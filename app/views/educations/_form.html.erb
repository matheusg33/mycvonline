<%= simple_form_for(@education) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :institution %>
    <%= f.input :course %>
    <%= f.input :level %>
    <%= f.input :started %>
    <%= f.input :ended %>
    <%= f.input :country %>
    <%= f.input :state %>
    <%= f.input :city %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
